    ],
    "faces": [
      {
        "id": "face_1753469837463_front",
        "main_text": "大漠孤烟直",
        "notes": "《使至塞上》 王维",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837463_back",
        "main_text": "长河落日圆",
        "notes": "描写边塞壮阔景色，适合表现辽阔与孤独",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837464_front",
        "main_text": "劝君更尽一杯酒",
        "notes": "《送元二使安西》 王维",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837464_back",
        "main_text": "西出阳关无故人",
        "notes": "惜别友人，表达离愁别绪",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837465_front",
        "main_text": "人生自是有情痴",
        "notes": "《玉楼春》 欧阳修",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837465_back",
        "main_text": "此恨不关风与月",
        "notes": "表达痴情与无奈，适合抒发情感",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837466_front",
        "main_text": "江流天地外",
        "notes": "《登江中孤屿》 谢灵运",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837466_back",
        "main_text": "山色有无中",
        "notes": "描绘江山辽阔，适合写景抒怀",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837467_front",
        "main_text": "黑发不知勤学早",
        "notes": "《劝学诗》 颜真卿",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837467_back",
        "main_text": "白首方悔读书迟",
        "notes": "强调学习要趁早，适合激励学生",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837468_front",
        "main_text": "纸上得来终觉浅",
        "notes": "《冬夜读书示子聿》 陆游",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837468_back",
        "main_text": "绝知此事要躬行",
        "notes": "强调实践的重要性，适合鼓励动手实践",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837469_front",
        "main_text": "天生我材必有用",
        "notes": "《将进酒》 李白",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837469_back",
        "main_text": "千金散尽还复来",
        "notes": "自信人生价值，适合自我激励",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837470_front",
        "main_text": "人生得意须尽欢",
        "notes": "《将进酒》 李白",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837470_back",
        "main_text": "莫使金樽空对月",
        "notes": "劝人及时行乐，珍惜当下",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837471_front",
        "main_text": "不畏浮云遮望眼",
        "notes": "《登飞来峰》 王安石",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837471_back",
        "main_text": "自缘身在最高层",
        "notes": "鼓励突破障碍，追求卓越",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837472_front",
        "main_text": "千磨万击还坚劲",
        "notes": "《竹石》 郑燮",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837472_back",
        "main_text": "任尔东西南北风",
        "notes": "比喻坚韧不拔的精神",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837473_front",
        "main_text": "沉舟侧畔千帆过",
        "notes": "《酬乐天扬州初逢席上见赠》 刘禹锡",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837473_back",
        "main_text": "病树前头万木春",
        "notes": "比喻新事物不断涌现，适合鼓励乐观",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837474_front",
        "main_text": "莫愁前路无知己",
        "notes": "《别董大》 高适",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837474_back",
        "main_text": "天下谁人不识君",
        "notes": "表达自信与乐观，适合送别或自勉",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837475_front",
        "main_text": "世事一场大梦",
        "notes": "《西江月》 苏轼",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837475_back",
        "main_text": "人生几度秋凉",
        "notes": "感叹人生短暂，适合哲理思考",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837476_front",
        "main_text": "不经一番寒彻骨",
        "notes": "《黄檗禅师诗》 释义",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837476_back",
        "main_text": "怎得梅花扑鼻香",
        "notes": "比喻经历磨难才能成功",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837477_front",
        "main_text": "相见时难别亦难",
        "notes": "《无题》 李商隐",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837477_back",
        "main_text": "东风无力百花残",
        "notes": "表达离别的无奈与伤感",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837478_front",
        "main_text": "此情可待成追忆",
        "notes": "《锦瑟》 李商隐",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837478_back",
        "main_text": "只是当时已惘然",
        "notes": "回忆往事，感叹时光流逝",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837479_front",
        "main_text": "洛阳亲友如相问",
        "notes": "《芙蓉楼送辛渐》 王昌龄",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837479_back",
        "main_text": "一片冰心在玉壶",
        "notes": "表达清白为人，适合送别",
        "created_at": "2025-07-25T18:57:17.463Z",
        "updated_at": "2025-07-25T18:57:17.463Z"
      },
      {
        "id": "face_1753469837480_front",
        "main_text": "山回路转不见君",
